import routeFactory, { type RecordT, type InsertT } from "{{importPathmap.api}}";

export default routeFactory(({ get, post }) => [
  get(async (ctx, worker): Promise<Array<RecordT>> => {
    return worker();
  }),

  post<never, { payload: InsertT }>(async (ctx, worker): Promise<RecordT> => {
    return worker(ctx.payload);
  }),
]);
