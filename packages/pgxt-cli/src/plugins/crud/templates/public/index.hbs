import { type RecordT, type InsertT, get, post } from "{{importPathmap.api}}";

export default [
  get(async (ctx, worker): Promise<Array<RecordT>> => {
    return worker();
  }),

  post<never, { payload: InsertT }>(async (ctx, worker): Promise<RecordT> => {
