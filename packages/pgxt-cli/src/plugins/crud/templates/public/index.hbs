import routeFactory, {
  type ListResponse,
  type CreateResponse,
  type InsertT,
} from "{{importPathmap.lib}}";

export default routeFactory(({ get, post }) => [
  get(async (ctx, worker): Promise<ListResponse> => {
    return worker();
  }),

  post<never, { payload: InsertT }>(async (ctx, worker): Promise<CreateResponse> => {
    return worker(ctx.payload);
  }),
]);
