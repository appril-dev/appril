{
  "name": "@appril/dbx-plugin-crud",
  "version": "0.0.48",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": ["pkg/*"],
  "exports": {
    ".": {
      "types": "./pkg/index.d.ts",
      "default": "./pkg/index.mjs"
    }
  },
  "scripts": {
    "// ✨ linters": "",
    "lint": "biome lint ./src",
    "// ✨ types": "",
    "tsc": "tsc",
    "// ✨ build": "",
    "prebuild:000": "rimraf pkg",
    "prebuild:001": "pnpm run lint",
    "prebuild:002": "pnpm run tsc",
    "prebuild": "set -e; for i in 0 1 2; do pnpm run prebuild:00${i}; done",
    "build": "node ../../esbuild.mjs index"
  },
  "devDependencies": {
    "@appril/configs": "workspace:^",
    "@types/fs-extra": "catalog:"
  },
  "dependencies": {
    "@appril/dbx": "workspace:^",
    "@appril/dbx-cli": "workspace:^",
    "@appril/dev": "workspace:^",
    "@appril/dev-utils": "workspace:^",
    "fs-extra": "catalog:",
    "smol-toml": "catalog:"
  }
}
