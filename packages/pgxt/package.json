{
  "name": "@appril/pgxt",
  "version": "0.0.0",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pkg/*"
  ],
  "exports": {
    ".": {
      "types": "./pkg/index.d.ts",
      "default": "./pkg/index.mjs"
    }
  },
  "scripts": {
    "// ✨ linters": "",
    "lint": "biome lint ./src",
    "// ✨ types": "",
    "tsc": "tsc",
    "// ✨ build": "",
    "prebuild:000": "rimraf pkg",
    "prebuild:001": "pnpm run lint",
    "prebuild:002": "pnpm run tsc",
    "prebuild": "set -e; for i in 0 1 2; do pnpm run prebuild:00${i}; done",
    "build": "../../esbuild.mjs index"
  },
  "dependencies": {
    "extract-pg-schema": "catalog:",
    "lodash-es": "catalog:",
    "pluralize": "^8.0.0",
    "zod": "catalog:zod"
  },
  "devDependencies": {
    "@appril/configs": "workspace:^",
    "@types/lodash-es": "catalog:",
    "@types/pg": "catalog:",
    "@types/pluralize": "^0.0.33"
  },
  "peerDependencies": {
    "pg": "catalog:"
  }
}
