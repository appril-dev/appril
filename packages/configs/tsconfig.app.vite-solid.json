    "paths": {
      "~/*": ["${configDir}/../*"],
      "@/*": ["${configDir}/*"],
      "_/*": ["${configDir}/var/*"]
    },
    "types": ["vite/client"],
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
