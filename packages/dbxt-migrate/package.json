{
  "name": "@appril/dbxt-migrate",
  "version": "0.0.6",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pkg/*"
  ],
  "bin": {
    "db-migrate": "pkg/cli.sh"
  },
  "scripts": {
    "// ✨ linters": "",
    "lint": "biome lint",
    "// ✨ types": "",
    "tsc": "tsc",
    "// ✨ build": "",
    "prebuild:000": "rimraf pkg",
    "prebuild:001": "pnpm run lint",
    "prebuild:002": "pnpm run tsc",
    "prebuild": "set -e; for i in 0 1 2; do pnpm run prebuild:00${i}; done",
    "build": "../../esbuild.mjs cli",
    "postbuild": "for e in *.sh; do cp --preserve=mode src/$e pkg; done"
  },
  "dependencies": {
    "@appril/configs": "workspace:^",
    "@appril/dev-utils": "workspace:^",
    "ansi-escapes": "catalog:",
    "date-fns": "catalog:",
    "esbuild": "catalog:",
    "fast-glob": "catalog:",
    "fs-extra": "catalog:",
    "kleur": "catalog:",
    "pg": "catalog:",
    "prompts": "catalog:"
  },
  "devDependencies": {
    "@types/fs-extra": "catalog:",
    "@types/pg": "catalog:",
    "@types/prompts": "catalog:"
  },
  "peerDependencies": {
    "knex": "catalog:"
  }
}
